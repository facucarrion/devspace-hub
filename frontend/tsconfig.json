{
  "extends": "astro/tsconfigs/strict",
  "exclude": ["node_modules", "dist", "public", "src/**/*.spec.ts", "src/**/*.test.ts"],
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@comp/*": ["src/components/*"],
      "@lib/*": ["src/lib/*"],
      "@layout/*": ["src/layouts/*"],
      "@types/*": ["src/types/*"],
    }
  },
}